{
  "name": "@tonnant/db",
  "exports": {
    "./*": {
      "types": "./dist/*.d.ts",
      "default": "./src/*.ts"
    }
  },
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "tsc",
    "build": "tsc",
    "clean": "rm -rf .turbo node_modules dist",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "deploy:db": "dotenv -e ../../.env.staging -- serverless deploy --verbose"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "3.437.0",
    "@aws-sdk/lib-dynamodb": "3.437.0",
    "@t3-oss/env-core": "^0.7.1",
    "dynamodb-toolbox": "^1.13.4",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@tonnant/tsconfig": "^0.1.0",
    "@types/jsonwebtoken": "^9.0.4",
    "dotenv-cli": "^7.3.0",
    "serverless": "~3.37.0",
    "typescript": "~5.7.2"
  }
}
