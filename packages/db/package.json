{
  "name": "@tonnant/db",
  "exports": {
    "./*": "./src/*.ts"
  },
  "typesVersions": {
    "*": {
      "*": [
        "src/*"
      ]
    }
  },
  "version": "0.1.0",
  "private": true,
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "tsc",
    "build": "tsc",
    "clean": "rm -rf .turbo node_modules",
    "typecheck": "tsc --noEmit",
    "deploy:db": "dotenv -e ../../.env.staging -- serverless deploy --verbose"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "3.437.0",
    "@aws-sdk/lib-dynamodb": "3.437.0",
    "@t3-oss/env-core": "^0.7.1",
    "dynamodb-toolbox": "^1.13.4",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@tonnant/tsconfig": "^0.1.0",
    "@types/jsonwebtoken": "^9.0.4",
    "dotenv-cli": "^7.3.0",
    "serverless": "~3.37.0",
    "typescript": "~5.3.3"
  }
}
